        ], 
        "dissolve": [
            "[value which]", 
            "[if {[value which]==0} {return NOFX} {return FX ON}][ knob tile_color [ expr {[ value which ]? 10289407 : 4278190335 } ] ]", 
            "IN = [value value_A] - [value value_B]\n OUT = [value value_C] - [value value_D]", 
            "[value input0.name]"
        ]
