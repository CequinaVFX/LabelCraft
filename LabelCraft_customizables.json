        ], 
        "switch": [
            "[value which]", 
            "Input 1 after [value value_A]", 
            "Input 0 before [value value_A]", 
            "Input = [value error]", 
            "[if {[value which]==0} {return NOFX} {return FX ON}]\n[ knob tile_color [ expr {[ value which ]? 10289407 : 4278190335 } ] ]"
        ], 
        "shuffle2": [
            "[value in1] > [value out1]", 
